{
  "name": "@googlemaps/react-native-driver-sdk",
  "version": "0.4.0",
  "author": "Google",
  "description": "A React Native library for Driver SDK on Google Maps Platform",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/googlemaps/react-native-driver-sdk.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/googlemaps/react-native-driver-sdk/issues"
  },
  "homepage": "https://github.com/googlemaps/react-native-driver-sdk#readme",
  "keywords": [
    "react-native",
    "ios",
    "android",
    "library",
    "google-navigation",
    "driver-sdk"
  ],
  "publishConfig": {
    "registry": "https://wombat-dressing-room.appspot.com"
  },
  "scripts": {
    "example:lmfs": "yarn workspace react-native-driver-sdk-lmfs-sample",
    "example:odrd": "yarn workspace react-native-driver-sdk-odrd-sample",
    "test": "jest --passWithNoTests",
    "test:types": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "bootstrap": "yarn install && yarn workspace react-native-driver-sdk-lmfs-sample pods && yarn workspace react-native-driver-sdk-odrd-sample pods",
    "clean": "del-cli example/LMFS/android/build example/LMFS/android/app/build example/LMFS/ios/build example/ODRD/android/build example/ODRD/android/app/build example/ODRD/ios/build lib",
    "prepare": "bob build",
    "build": "bob build"
  },
  "dependencies": {
    "@googlemaps/react-native-navigation-sdk": "^0.12.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.2.2",
    "@evilmartians/lefthook": "^1.5.0",
    "@react-native/babel-preset": "^0.81.1",
    "@react-native/typescript-config": "^0.81.1",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.5",
    "@types/react": "19.1.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "commitlint": "^19.3.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-testing-library": "^6.2.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "react": "19.1.0",
    "react-native": "0.81.1",
    "react-native-builder-bob": "^0.40.13",
    "react-test-renderer": "^19.1.0",
    "turbo": "^1.10.7",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "workspaces": [
    "example/LMFS",
    "example/ODRD"
  ],
  "packageManager": "yarn@3.6.4",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/LMFS/node_modules",
      "<rootDir>/example/ODRD/node_modules",
      "<rootDir>/lib/"
    ],
    "moduleNameMapper": {
      "^react-native$": "<rootDir>/node_modules/react-native",
      "^@googlemaps/react-native-navigation-sdk$": "<rootDir>/node_modules/@googlemaps/react-native-navigation-sdk"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
